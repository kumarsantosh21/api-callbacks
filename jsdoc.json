{
    "source": {
        "include": [ "./src","./readme.md" ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },

    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks":true,
        "default":{
            "outputSourceFiles":false
        }
    },

    "opts": {                
        "destination": "./docs/",          
        "recurse": true
                 
       
    }


}